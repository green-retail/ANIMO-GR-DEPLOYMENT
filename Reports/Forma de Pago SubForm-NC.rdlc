  <DataSets>
    <DataSet Name="DS_TiposIngresos_nc">
      <Query>
        <DataSourceName>AnimoGreenRetail</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Fecha">
            <Value>=Parameters!Fecha.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Metodo, SUM(Montoxtipo) AS monto, 
                         CASE WHEN Metodo = '01' THEN 'Efectivo' WHEN Metodo = '02' THEN 'Visa' WHEN Metodo = '03' THEN 'Clave' WHEN Metodo = '09' THEN 'Master' WHEN Metodo = '10' THEN 'Amex' END AS Descripcion
FROM            (SELECT        CASE WHEN [Code Payment Metodth] = '08' THEN OriginalPaymentMethod WHEN [Code Payment Metodth] = '02' THEN Reference ELSE [Code Payment Metodth] END AS Metodo, 
                                                    CAST(SUM(sp.Amount) AS decimal(18, 2)) AS Montoxtipo
                          FROM            [Sales Payment methods] AS sp INNER JOIN
                                                    [Sales Header] AS hd ON hd.[No Transaccion] = sp.[Transaction No]
                          WHERE        (hd.[Document Type] = 9) AND (hd.Status = 4 OR
                                                    hd.Status = 5) AND (sp.State = 1) AND (CONVERT(date, hd.[Document Date], 103) = CONVERT(date, @Fecha, 103))
                          GROUP BY sp.[Code Payment Metodth], sp.Reference, sp.OriginalPaymentMethod) AS a
GROUP BY Metodo
ORDER BY Metodo</CommandText>
      </Query>
      <Fields>
        <Field Name="Metodo">
          <DataField>Metodo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="monto">
          <DataField>monto</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Descripcion">
          <DataField>Descripcion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
